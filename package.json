{
  "name": "@adobe/helix-media-log",
  "version": "1.1.3",
  "private": true,
  "description": "Helix Media Log",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "c8 mocha  -i -g 'Post-Deploy'",
    "test-postdeploy": "mocha -g 'Post-Deploy'",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH",
    "commit": "git-cz",
    "build": "hedy -v",
    "deploy": "hedy -v --deploy --test",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci --cleanup-ci 24h",
    "prepare": "husky"
  },
  "wsk": {
    "nodeVersion": 22,
    "name": "helix3/media-log@${version}",
    "memory": 1024,
    "#timeout": "5 minutes",
    "timeout": "300000",
    "awsRole!important": "arn:aws:iam::118435662149:role/helix-service-role-s3-rw",
    "testUrl": "/_status_check/healthcheck.json",
    "awsAttachAuthorizer": "helix-token-authorizer_v2",
    "target": "aws",
    "fastlyServiceId!important": ""
  },
  "mocha": {
    "spec": "test/**/*.test.js",
    "require": "test/setup-env.js",
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-media-log"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-media-log/issues"
  },
  "homepage": "https://github.com/adobe/helix-media-log#readme",
  "dependencies": {
    "@adobe/fetch": "4.2.3",
    "@adobe/helix-admin-support": "3.4.2",
    "@adobe/helix-shared-body-data": "2.2.3",
    "@adobe/helix-shared-process-queue": "3.1.5",
    "@adobe/helix-shared-secrets": "2.3.0",
    "@adobe/helix-shared-tokencache": "1.5.0",
    "@adobe/helix-shared-utils": "3.0.2",
    "@adobe/helix-shared-wrap": "2.0.2",
    "@adobe/helix-status": "10.1.5",
    "@aws-sdk/client-s3": "3.966.0",
    "@aws-sdk/client-sqs": "3.966.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.17",
    "@adobe/helix-deploy": "13.2.11",
    "@adobe/helix-universal": "5.4.0",
    "@adobe/helix-universal-devserver": "1.1.145",
    "@adobe/semantic-release-coralogix": "1.1.40",
    "@eslint/config-helpers": "0.5.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "c8": "10.1.3",
    "dotenv": "17.2.3",
    "eslint": "9.4.0",
    "esmock": "2.7.3",
    "husky": "9.1.7",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.2.7",
    "mocha": "11.7.5",
    "mocha-multi-reporters": "1.5.1",
    "mocha-suppress-logs": "0.6.0",
    "nock": "13.5.6",
    "nodemon": "3.1.11",
    "semantic-release": "24.2.9",
    "sinon": "21.0.1",
    "xml2js": "0.6.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.cjs": "eslint"
  }
}
