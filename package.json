{
  "name": "@adobe/helix-media-log",
  "version": "1.0.0",
  "private": true,
  "description": "Helix Media Log",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon",
    "test": "c8 mocha  -i -g 'Post-Deploy' --spec=test/**/*.test.js",
    "test-postdeploy": "mocha -g 'Post-Deploy' --spec=test/**/*.test.js",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "build": "hedy -v",
    "deploy": "hedy -v --deploy --test",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci --cleanup-ci 24h",
    "prepare": "husky"
  },
  "wsk": {
    "nodeVersion": 22,
    "name": "helix3/media-log@${version}",
    "memory": 1024,
    "#timeout": "5 minutes",
    "timeout": "300000",
    "awsRole!important": "arn:aws:iam::118435662149:role/helix-service-role-s3-rw",
    "testUrl": "/_status_check/healthcheck.json",
    "awsAttachAuthorizer": "helix-token-authorizer_v2",
    "target": "aws",
    "fastlyServiceId!important": "",
    "dev": {
      "params-file": "secrets/dev-secrets.json"
    }
  },
  "mocha": {
    "require": [
      "test/setup-env.js",
      "mocha-suppress-logs"
    ],
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-media-log"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-media-log/issues"
  },
  "homepage": "https://github.com/adobe/helix-media-log#readme",
  "dependencies": {
    "@adobe/fetch": "4.2.3",
    "@adobe/helix-admin-support": "3.4.2",
    "@adobe/helix-shared-body-data": "2.2.1",
    "@adobe/helix-shared-process-queue": "3.1.3",
    "@adobe/helix-shared-secrets": "2.2.10",
    "@adobe/helix-shared-tokencache": "1.5.0",
    "@adobe/helix-shared-utils": "3.0.2",
    "@adobe/helix-shared-wrap": "2.0.2",
    "@adobe/helix-status": "10.1.5",
    "@aws-sdk/client-s3": "3.901.0",
    "@aws-sdk/client-sqs": "3.901.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.12",
    "@adobe/helix-deploy": "13.1.19",
    "@adobe/helix-universal": "5.2.3",
    "@adobe/helix-universal-devserver": "1.1.141",
    "@adobe/semantic-release-coralogix": "1.1.40",
    "@eslint/config-helpers": "0.4.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "c8": "10.1.3",
    "dotenv": "17.2.3",
    "eslint": "9.4.0",
    "esmock": "2.7.3",
    "husky": "9.1.7",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.2.3",
    "mocha": "11.7.4",
    "mocha-multi-reporters": "1.5.1",
    "mocha-suppress-logs": "0.6.0",
    "nock": "13.5.6",
    "nodemon": "3.1.10",
    "semantic-release": "24.2.9",
    "sinon": "21.0.0",
    "xml2js": "0.6.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.cjs": "eslint"
  },
  "engines": {
    "node": ">= 14.17"
  },
  "nodemonConfig": {
    "exec": "node --inspect ./test/dev/server.mjs",
    "watch": [
      ".env",
      "."
    ],
    "ext": ".js, .cjs, .ejs, .css"
  }
}
